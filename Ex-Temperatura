#include<stdio.h>

int main() {
    float temp;
    char unidade;
    scanf("%f %c", &temp, &unidade);
    
    if (unidade=='c'){
        
         if(temp<35){
             printf("Hipotermia");
             else
         
           if(temp<36,5) {
               printf("Em risco");
               else
         
               if(temp<37,5) {
                  printf("Normal");
                  else
         
                  if(temp<40) {
                     printf("Em risco");     
                     else

                     printf("Hiperpirexia");
                     
              { 
            {   
          {
        { 
      {
            
            
    if (unidade=='f'){
        
        if(temp<95) {
             printf("Hipotermia");    
        }
        
        if(95<=temp<97,7) {
            printf("Em risco");
        }
        
        if(97,7<=temp<99,5) {
            printf("Normal");
        }
        
        if(99,5<=temp<104) {
            printf("Em risco");
        }
        
        if(104<=temp) {
            printf("Hiperpirexia")
        }
    }
    return 0;
}
